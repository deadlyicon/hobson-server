%h1 Project Test Runs

%table
  %thead
    %th ID
    %th sha
    %th created_at
    %th
  %tbody
    - test_runs.each do |test_run|
      %tr
        %td
          %a{href: test_run_path(test_run)}= test_run['id']
        %td
          = test_run['sha']
        %td
          = test_run['created_at']
        %td
          - delete_link 'X', test_run_path(test_run)
